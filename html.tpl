<!doctype html>

<html lang='{langue}'>

	<head>

		<meta charset="utf-8">

		<link rel="shortcut icon" type="image/png" href="{icone}" />

		<title>{titre}</title>

		<meta name="description" content="{description}" />
		<meta name="keywords" content="{keywords}" />

		<meta name="author" content="contac[a]data4you.fr" />
		<meta name="copyright" content="data4you.fr" />

		<meta property="og:title" content="{titre}" />
		<meta property="og:type" content="website" />
		<meta property="og:image" content="{image}" />
		<meta property="og:url" content="{urlPage}" />
		<meta property='og:site_name' content='{titre}'>
		<meta property='og:locale' content='{langueLocal}'>
		<meta property="og:description" content="{description}" />

		<meta name="twitter:card" content="summary" />
		<meta name="twitter:title" content="{titre}" />
		<meta name="twitter:description" content="{description}" />
		<meta name="twitter:image" content="{image}" />

		<meta name="viewport" content="width=device-width,initial-scale=1.0,user-scalable=no" /> 
		
		<!-- BEGIN css -->
		
			<link type="text/css" rel="stylesheet" media="all" href="{css.src}" />
		
		<!-- END css -->
		
	</head>

	<body data-url='{url}' data-link='{urlPage}'>

		{body}
		
		<!-- BEGIN javascript -->
		
			<script type="text/javascript" src="{javascript.src}"></script>
		
		<!-- END javascript -->
		
		{googleAnalytics}
		
	</body>

</html>
